{
  "index_patterns": ["vector-journald-*", "vector-docker-*"],
  "priority": 200,
  "template": {
    "settings": {
      "index": {
        "number_of_shards": 6,
        "number_of_replicas": 1,
        "refresh_interval": "30s"
      }
    },
    "mappings": {
      "dynamic": "strict",
      "_source": {
        "excludes": ["log_parsed"]
      },       
      "properties": {
        "@timestamp": { "type": "date" },
        "message": {
          "type": "text",
          "fields": { "keyword": { "type": "keyword" } }
        },
        "level": { "type": "keyword" },
        "hostname": { "type": "keyword" },
        "unit": { "type": "keyword" },
        "pid": { "type": "keyword" },
        "container_id": { "type": "keyword" },
        "container_name": { "type": "keyword" },
        "image": {  "type": "keyword"},
        "source": { "type": "keyword" },
        "log_type": { "type": "keyword" },
        "cluster_name": { "type": "keyword" },
        "node_name": { "type": "keyword" },
        "http_method": { "type": "keyword" },
        "http_status": { "type": "integer" },
        "http_path": { "type": "keyword" },
        "http_host": { "type": "keyword" },
        "http_scheme" : { "type": "keyword" },
        "client_ip": { "type": "keyword" },
        "response_time_ms": { "type": "float" },
        "full_url": {"type": "keyword"},

        "log_parsed": { 
            "type": "object",
            "enabled": false   
        }
      }
    }
  }
}